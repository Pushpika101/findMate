{
  "expo": {
  "name": "findMate",
    "slug": "lost-and-found",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "lostandfound",
    "splash": {
  "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4F46E5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pdn.lostandfound"
    },
    "android": {
      "adaptiveIcon": {
  "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#4F46E5"
      },
      "package": "com.pdn.lostandfound"
    },
    "web": {
  "favicon": "./assets/images/favicon.png"
    },
    "plugins": [],
    "extra": {
      "apiUrl": "http://192.168.1.111:5001/api",
      "socketUrl": "http://192.168.1.111:5001"
    }
  }
}